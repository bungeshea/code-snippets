{
    "name": "sheabunge/code-snippets-pro",
    "description": "Manage code snippets running on a WordPress-powered site through a graphical interface",
    "keywords": [
        "wordpress",
        "snippets",
        "organization"
    ],
    "type": "wordpress-plugin",
    "homepage": "https://wordpress.org/plugins/code-snippets",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Shea Bunge",
            "email": "shea@codesnippets.pro",
            "homepage": "https://sheabunge.com"
        }
    ],
    "support": {
        "issues": "https://github.com/sheabunge/code-snippets/issues",
        "source": "https://github.com/sheabunge/code-snippets"
    },
    "autoload": {
        "classmap": [
            "php/"
        ]
    },
    "require": {
        "composer/installers": "~1.0",
        "php": ">=5.6",
        "ext-dom": "*",
        "ext-json": "*",
        "matthiasmullie/minify": "^1.3"
    },
    "require-dev": {
        "wp-coding-standards/wpcs": "dev-master",
        "phpcompatibility/phpcompatibility-wp": "*",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
